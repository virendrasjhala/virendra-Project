	if(n9<8)
	  {
		fprintf(pf,"%s\t%d\t%d\t%d*\n",n8,n9,n18,a4);
	  }
	else
	  {
		fprintf(pf,"%s\t%d\t%d\t%d\n",n8,n9,n18,a4);
	  }
	printf("Enter 5th subject\n");
	scanf("%s",&n10);
	printf("Enter internal marks & external marks\n");
	scanf("%d%d",&n11,&n19);
	a5=n11+n19;
	if(n11<8)
	  {
		fprintf(pf,"%s\t%d\t%d\t%d*\n",n10,n11,n19,a5);
	  }
	else
	  {
		fprintf(pf,"%s\t%d\t%d\t%d\n",n10,n11,n19,a5);
	  }
	printf("Enter 6th subject\n");
	scanf("%s",&n12);
	printf("Enter internal mrks & external marks\n");
	scanf("%d%d",&n13,&n20);
	a6=n13+n20;
	if(n13<8)
	  {
		fprintf(pf,"%s\t%d\t%d\t%d*\n",n12,n13,n20,a6);
	  }
	else
	  {
		fprintf(pf,"%s\t%d\t%d\t%d\n",n12,n13,n20,a6);
	  }
	total1=(n2+n5+n7+n9+n11+n13);
	total2=(n15+n16+n17+n18+n19+n20);
	gtotal=total1+total2;
	percentage=((gtotal*100)/600);
	 if((n2<8) || (n5<8) || (n7<8) || (n9<8) || (n11<8) || (n13<8))
	     {
		  fprintf(pf,"internal back\t%d\n",total1);
		  if((n15<32) || (n16<32) || (n17<32) || (n18<32) || (n19<32) || (n20<32))
		    {
			  fprintf(pf,"external back\t%d\ntotal\t%d\npercentage\t%f\n",total2,gtotal,percentage);
		    }
	     }
	 else
	     {
		  fprintf(pf,"internal clear\t%d\n",total1);
		  if((n15<32) || (n16<32) || (n17<32) || (n18<32) || (n19<32) || (n20<32))
		    {
		       fprintf(pf,"external back\t%d\ntotal\t%d\npercentage\t%f\n",total2,gtotal,percentage);
		    }
		  else
		    {
		       fprintf(pf,"internal clear\t%d\nexternal clear\t%d\ntotal\t%d\npercentage\t%f\n",total1,total2,gtotal,percentage);
		    }
	     }
      fclose(pf);
      getch();
}
