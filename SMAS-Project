void entry6();
void main()
{
   int ch;
   clrscr();
   while(1)
   {
      printf("                        -------------------------------\n                             |marks analysis system|\n                        -------------------------------\n");
      printf("                              Enter your choice\n                         ------------------------------\n                              1)teacher \n                              2)student \n                              3)exit\n                         ------------------------------\n");
      scanf("%d",&ch);
      switch(ch)
      {
	  case 1: teacher();
		  break;
	  case 2: student();
	     break;
	  default: exit();
       }
   }
}
void teacher()
{
  clrscr();
  pass();
}
void student()
{

   char name[20]="virendra",pass[20]="13etccs039",name1[20],pass1[20],p;
   char st1[20]="mohit",st1_p[20]="13etccs040",st2[20]="niteen",st2_p[20]="13etccs041";
   char st3[20]="sachin",st3_p[20]="13etccs042",st4[20]="vipul",st4_p[20]="13etccs043";
   int i=0;
   clrscr();
   printf("                         ------------------------------\n                              enter student id\n                         ------------------------------\n");
   scanf("%s",&name1);
   printf("                         ------------------------------\n                              Enter roll_no\n                         ------------------------------\n");
   while((p=getch())!=13)
	{
	  pass1[i++]=p;
	  printf("*");
	}
   pass1[i]=NULL;
   if(!strcmp(st1,name1) && !strcmp(st1_p,pass1))
	{
	    file2();
	}
   else
	{
	    if(!strcmp(st2,name1) && !strcmp(st2_p,pass1))
	      {
		 file3();
	      }
	    else
	      {
		 if(!strcmp(st3,name1) && !strcmp(st3_p,pass1))
		    {
			file4();
		    }
		 else
		    {
		      if(!strcmp(st4,name1) && !strcmp(st4_p,pass1))
			{
			    file5();
			}
		    }
	      }
	}
   if(strlen(name1)<5)
      {
	 printf("                         -----------------------------\n                              password is too short please enter long\n                         ------------------------------\n");
       }
   else
      {

	  if(!strcmp(pass,pass1) && !strcmp(name,name1))
	      {
		  printf("                         ------------------------------\n                             you have successfuly logged\n                         ------------------------------\n");
		  s_file();
	      }
	 else
	      {
		  printf("                         ------------------------------\n                             wrong password plz re-enter\n                        -----------------------------\n");

	      }
      }


}
void s_file()
{

    FILE *pf,*fopen();
    char c;
    clrscr();
    pf=fopen("entry1.txt","r");
    do
      {
	 c=getc(pf);
	 putchar(c);
      }
     while(c!=EOF);
     fclose(pf);

}
